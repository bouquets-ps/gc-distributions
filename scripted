}
[data-theme="light"] {
   --color-bg: #ffffff;
   --color-fg: #000000;
  --transition-speed: 0.5s;
}
[data-theme="dark"] {
   --color-bg: #000000;
   --color-fg: #ffffff;
  --transition-speed: 0.5s;
}
button {
  font-family: "Quicksand", sans-serif;
  border: 1px solid var(--color-fg);
  border-radius: 5px;
  padding: 0.5rem 1rem;
